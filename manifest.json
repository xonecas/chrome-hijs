{
   "name": "Hijs",
   "version": "0.0.1",
   "description": "Raw javascript file highlighter.",
   "permissions" : [
      "http://*/*.js",
      "https://*/*.js"
   ],
   "content_scripts": [
      {
         "matches": ["http://*/*.js", "https://*/*.js"],
         "js": ["hijs.js"],
         "css": ["highlight.css"]
      }
   ]
}

